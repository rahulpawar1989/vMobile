{
  "expo": {
    "name": "vMobile",
    "slug": "vMobile-react-project",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/vmobilelogohdpi.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.glcomm.vMobile",
      "infoPlist": {
        "NSLocationAlwaysUsageDescription": "Allow Expo experiences to use your location"
      }
    },
    "android": {
      "package": "com.glcomm.vMobile"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  },
  "displayName": "vMobile",
  "name": "vMobile"
}